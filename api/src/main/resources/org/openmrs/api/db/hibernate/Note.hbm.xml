<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.1//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.openmrs.notification">

	<class name="org.openmrs.notification.Note" table="notification_note"  batch-size="25">
		<id name="noteId" type="int" column="note_id" unsaved-value="0" >
			<generator class="native">
				<param name="sequence">notification_note_note_id_seq</param>
			</generator>
		</id>
		<discriminator column="note_id" insert="false" />
		
			<property name="noteId" type="java.lang.Integer" length="11" column="note_id" not-null="true" insert="false" update="false" />
			<property name="text" type="java.lang.String" column="text" not-null="true" length="512" />
			<many-to-one name="creator" class="org.openmrs.User" column="creator" not-null="true" />
			<property name="dateCreated" type="java.util.Date" column="date_created" not-null="true" length="19" />
			<many-to-one name="changedBy" class="org.openmrs.User" column="changed_by" />
			<property name="dateChanged" type="java.util.Date" column="date_changed" length="19" />
			<property name="voided" type="java.lang.Boolean" column="is_voided" not-null="true" length="1" />
			<property name="dateVoided" type="java.util.Date" column="date_voided" not-null="true" length="19" />
			<many-to-one name="voidedBy" class="org.openmrs.User" column="voided_by" not-null="true" />
			<property name="voidReason" type="java.lang.String" column="void_reason" length="255" />
			<property name="priority" type="java.lang.Integer" length="11" column="priority" not-null="true" />
			<property name="weight" type="java.lang.Integer" length="11" column="weight" not-null="true" />
			<property name="uuid" type="java.lang.String" column="uuid" length="38" unique="true" />	
			<property name="patient" type="org.openrms.Patient" column="patient" not-null="true" />
			<property name="noteType" type="org.openmrs.notification.NoteType" column="note_type" not-null="true" />
	</class>
	
</hibernate-mapping>